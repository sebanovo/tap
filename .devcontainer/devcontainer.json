{
  "name": "tap",
  "dockerComposeFile": "../docker-compose.dev.yml",
  "service": "dev-container",
  "initializeCommand": "docker compose -f ./docker-compose.dev.yml down -v",
  "workspaceFolder": "/workspaces",

  "postAttachCommand": "chmod +x ./entrypoint.dev.sh && /bin/bash -lc ./entrypoint.dev.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint",
        "bradlc.vscode-tailwindcss",
        "yoavbls.pretty-ts-errors",
        "prisma.prisma"
      ]
    }
  }
}
